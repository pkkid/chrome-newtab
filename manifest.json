{
  "manifest_version": 2,
  "name": "PKs New Tab Tools",
  "short_name": "PKs New Tab Tools",
  "version": "2021.03.22",
  "description": "PKs New Tab Tools",
  "permissions": ["storage", "tabs"],
  "options_page": "options.html",
  "chrome_url_overrides": {"newtab": "newtab.html"},
  "browser_action": {
    "default_icon": {
      "128": "img/newtab128.png",
      "64": "img/newtab64.png",
      "32": "img/newtab32.png"
    }
  },
  "incognito": "split",
  "minimum_chrome_version" : "22"
}
